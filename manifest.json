{
    "manifest_version": 2,
    "name": "Whitelist",
    "description": "Only make requests to a fixed set of domains.",
    "version": "0.1.0",

    "browser_action": {
      "default_popup": "popup.html"
    },

    "background": {
      "scripts": ["background.js"]
    },
    "permissions": [
      "storage",
      "webRequest",
      "webRequestBlocking",
      "<all_urls>"
    ]
}
